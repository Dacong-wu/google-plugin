{
  "name": "大葱Ding",
  "description": "快回消息，Ding!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "assets/16.png",
    "32": "assets/32.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["notifications"],
  "minimum_chrome_version": "116",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
